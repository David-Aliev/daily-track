{
  "name": "daily-track",
  "version": "0.0.0",
  "description": "daily-tracker",
  "scripts": {
    "go": "npm-run-all --serial install-server install-client dev",
    "install-server": "npm i --prefix server",
    "install-client": "npm i --prefix client",
    "dev": "concurrently \"npm run dev --prefix server\" \"npm run dev --prefix client\""
  },
  "author": "Monobladegg",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "npm-run-all": "^4.1.5"
  }
}
